{
  "name": "confusion",
  "version": "1.0.0",
  "description": "This is a website for Ristorante Con Fusion",
  "main": "index.html",
  "scripts": {
    "start": "npm run watch:all",
    "compile:less": "lessc css/styles.less css/styles.css",
    "compile:scss": "node-sass css/styles.scss css/styles.css",
    "watch:scss": "onchange \"css/*.scss\" -- npm run compile:scss",
    "watch:all": "parallelshell \"npm run watch:scss\" \"npm run lite\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lite": "lite-server"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "lite-server": "^2.3.0",
    "node-sass": "^9.0.0",
    "onchange": "^7.1.0"
  },
  "dependencies": {
    "bootstrap": "^4.0.0",
    "bootstrap-social": "^5.1.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "parallelshell": "^3.0.1",
    "popper.js": "^1.12.9"
  }
}
